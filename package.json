  "scripts": {
    "app": "better-npm-run webpack-dev-server",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "clean": "rimraf build/*",
    "flow": "flow",
    "lint": "npm-run-all lint:css lint:js",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint --config .eslintrc src",
    "test": "jest --config config/jest/jest.config.js",
    "test:bamboo": "better-npm-run test:bamboo",
    "test:karma": "karma start config/karma/karma.config.js",
    "test:watch": "jest --config config/jest/jest.config.js --watch"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production"
      }
    },
    "webpack-dev-server": {
      "command": "webpack-dev-server --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "development"
      }
    },
    "test:bamboo": {
      "command": "karma start config/karma/karma.config.js",
      "env": {
        "TEST": "bamboo"
      }
    }
  }
